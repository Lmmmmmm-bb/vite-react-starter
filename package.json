{
  "name": "vite-react-starter",
  "packageManager": "pnpm@8.10.2",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host & pnpm style:watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "style:watch": "typed-scss-modules ./src/**/index.module.scss --watch"
  },
  "dependencies": {
    "@unocss/reset": "^0.57.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@lmmmmmm/eslint-config": "^0.3.0",
    "@lmmmmmm/eslint-config-react": "^0.3.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@unocss/preset-attributify": "^0.57.2",
    "@unocss/transformer-attributify-jsx": "^0.57.2",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "sass": "^1.69.5",
    "typed-scss-modules": "^7.1.4",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "vite": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint . --fix"
  }
}
