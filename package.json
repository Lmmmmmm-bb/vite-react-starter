{
  "name": "vite-react-starter",
  "packageManager": "pnpm@8.12.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host & pnpm style:watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "style:watch": "typed-scss-modules ./src/**/index.module.scss --watch"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@lmmmmmm/eslint-config": "^0.3.0",
    "@lmmmmmm/eslint-config-react": "^0.3.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@unocss/preset-attributify": "^0.58.0",
    "@unocss/transformer-attributify-jsx": "^0.58.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "sass": "^1.69.5",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "vite": "^5.0.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint . --fix"
  }
}
