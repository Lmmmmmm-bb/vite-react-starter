  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@lmmmmmm/eslint-config": "^0.1.0",
    "@lmmmmmm/eslint-config-react": "^0.1.0",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "typed-scss-modules": "^7.1.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint . --fix"
