{
  "name": "vite-react-starter",
  "packageManager": "pnpm@8.15.4",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host & pnpm style:watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "prepare": "husky install",
    "style:watch": "typed-scss-modules ./src/**/index.module.scss --watch"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@lmmmmmm/eslint-config": "^0.3.0",
    "@lmmmmmm/eslint-config-react": "^0.3.0",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "@unocss/preset-attributify": "^0.58.5",
    "@unocss/transformer-attributify-jsx": "^0.58.5",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "sass": "^1.72.0",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "vite": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint . --fix"
  }
}
